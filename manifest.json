{
  "name": "Wide Twitch Clips",
  "version": "0.1",
  "description": "Custom CSS injection into clips.twitch.tv",
  "content_scripts": [
    {
      "matches": ["https://clips.twitch.tv/*", "http://clips.twitch.tv/*"],
      "css": ["styles.css"]
    }
  ],
  "icons": { "16": "/icon/icon16.png",
    "32": "/icon/icon32.png",
   "128": "/icon/icon128.png" },
  "manifest_version": 2
}